{
  "name": "palworld-wiki",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jrposada/palworld-wiki.git"
  },
  "workspaces": [
    "backend",
    "frontend",
    "shared"
  ],
  "scripts": {
    "publish:docker": ". ./scripts/publish.sh",
