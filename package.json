{
  "name": "palworld-wiki",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jrposada/palworld-wiki.git"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "prettier . --write",
    "start:api": "node ./src/api/index.js",
    "start:db": "docker compose --file compose-db.yml up --detach",
    "start": "docker compose --file compose.yml up --detach",
    "stop": "docker compose --file compose.yml stop"
  },
  "author": {
    "email": "jrposada.dev@gmail.com",
    "name": "Javier Rodriguez Posada"
  },
  "type": "module",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4"
  }
}
